{
  "name": "randomguy400/intelligent-moderation",
  "timeout": 20000,
  "connector": false,
  "events": {
    "functions/events/discord/mute.js": {
      "name": "discord.command",
      "subtype": {
        "command": "mute"
      }
    },
    "functions/events/discord/unmute.js": {
      "name": "discord.command",
      "subtype": {
        "command": "unmute"
      }
    },
    "functions/events/discord/kick.js": {
      "name": "discord.command",
      "subtype": {
        "command": "kick"
      }
    },
    "functions/events/discord/message/message_filter.js": {
      "name": "discord.message.create"
    },
    "functions/events/discord/ban.js": {
      "name": "discord.command",
      "subtype": {
        "command": "ban"
      }
    },
    "functions/events/discord/warn.js": {
      "name": "discord.command",
      "subtype": {
        "command": "warn"
      }
    },
    "functions/events/discord/message/create.js": {
      "name": "discord.message.create"
    }
  },
  "env": [
    {
      "name": "warns_mute",
      "description": "How many warns a user gets until they are auto-muted"
    },
    {
      "name": "warns_kick",
      "description": "How many warns a user gets until they are auto-kicked"
    },
    {
      "name": "warns_ban",
      "description": "How many warns a user gets until they are auto-banned"
    },
    {
      "name": "main_role",
      "description": "The name of the main role that a user uses to talk in the server"
    },
    {
      "name": "mute_role",
      "description": "The name of the muted role used in the server"
    }
  ]
}